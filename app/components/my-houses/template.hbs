{{#each housesData as |house|}}

    <div class="row all-property-card">
      <div class="col s12 l10">
        <div class="card white darken-1">
          <div class="card-content house-card">
            {{#link-to 'house' house.id}}
            <span id="house-name-card-title" class="card-title">{{house.street_name}}</span>
            {{/link-to}}

            <div class="divider">
            </div>

            <div class="house-quick-info-container row">
              <div class="col s6 m4">
                <div class="card-panel">
                  <span class="house-quick-info ">
                    <p>$12,000</p>
                  </span>
                  <span class="subscript">
                    <p>Net Revenue</p>
                  </span>
                </div>
              </div>

              <div class="col s6 m4">
                <div class="card-panel">
                  <span class="house-quick-info white">
                    <p>5/6</p>
                  </span>
                  <span class="subscript">
                    <p>Active Units</p>
                  </span>
                </div>
              </div>

              <div class="col s6 m4">
                <div class="card-panel">
                  <span class="house-quick-info ">
                    <p>$12,000</p>
                  </span>
                  <span class="subscript">
                    <p>Net Revenue</p>
                  </span>
                </div>
              </div>

              <div class="col s6 m4">
                <div class="card-panel">
                  <span class="house-quick-info white">
                    <p>5/6</p>
                  </span>
                  <span class="subscript">
                    <p>Active Units</p>
                  </span>
                </div>
              </div>

            </div><!-- house-quick-info-container row -->
          </div><!-- card-content -->
        </div><!--card-->
      </div><!--col-->

      <div class="col s12 l2">
        {{#if house.units}}
        {{#each house.units as |unit|}}
          {{#link-to "unit" unit.id}}
          <div class="card z-depth-2">
            <div class="unit-card-content center-align flow-text">
              <span class="unit-subscript">Unit</span> {{unit.unit_num}}
            </div>
          </div>
          {{/link-to}}
        {{/each}}
        {{/if}}
      </div>

</div><!--row-->
{{/each}}
