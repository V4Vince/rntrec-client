<div class="row valign-wrapper">
  <div class="col s2">
    <div class="section-title" {{action "openMoneyDetails"}}><a>Money</a></div>
  </div>
  <div class="col s10">
    <p class="right-align flow-text">No notifications</p>
  </div>
</div>

<div class="divider"></div>


<div class="row center-align property-card-sections">
  <div class="container">
    <!-- Revenue -->
    <div class="col s6 display-flex">
      <div class="row valign-wrapper">
        <div>
          <h4>{{format-currency unitData.totalExpenses}}</h4>
          <span>Revenue To Date</span>
        </div>

          {{md-btn action='addExpense'
            icon='play_for_work'
            buttonType='floating'
            class='teal
            hide-on-small-only
            circle-add-buttons' }}
      </div>
    </div>
    <!-- Expense -->
    <div class="col s6 display-flex">
      <div class="row valign-wrapper">
        <div>
          <h4>{{format-currency unitData.totalExpenses}}</h4>
          <span>Expense To Date</span>
        </div>
        <div>
          {{md-btn action='addExpense'
            icon='play_for_work'
          	buttonType='floating'
          	class='teal
            hide-on-small-only
            circle-add-buttons' }}
        </div>
      </div>
    </div>

  </div>
</div>
