<div class="row">
{{md-input-date label='Lease Start'
    value=contractParams.start
    class="col s12 m3"
    numberOfYears=5 }}

{{md-input-date label='Lease End'
    value=contractParams.end
    class="col s12 m3"
    numberOfYears=5 }}
</div>

<div class="row">
{{md-input value=contractParams.num_tenants
    label="Number of tenants"
    type='number'
    class="col s6"
    validate=true}}
</div>
<div class="row">
{{md-input value=contractParams.rent
    label="Rent Amount"
    type='number'
    class="col s6"
    validate=true}}
</div>
<div class="row">
{{md-input value=contractParams.security
    label="Security Deposit"
    type='number'
    class="col s6"
    validate=true}}
</div>
